[
  {
    "name": "api-deployment",
    "schema_name": "Kubernetes Deployment",
    "attributes": {
      "replicas": 3,
      "selector": {
        "matchLabels": {
          "app": "api-service"
        }
      },
      "template": {
        "metadata": {
          "labels": {
            "app": "api-service",
            "version": "v1.2.0"
          }
        },
        "spec": {
          "containers": [
            {
              "name": "api",
              "image": "mycompany/api-service:v1.2.0",
              "ports": [
                {
                  "containerPort": 8080,
                  "name": "http"
                }
              ],
              "env": [
                {
                  "name": "DATABASE_URL",
                  "valueFrom": {
                    "secretKeyRef": {
                      "name": "api-secrets",
                      "key": "database-url"
                    }
                  }
                }
              ],
              "resources": {
                "requests": {
                  "memory": "256Mi",
                  "cpu": "250m"
                },
                "limits": {
                  "memory": "512Mi",
                  "cpu": "500m"
                }
              }
            }
          ]
        }
      }
    },
    "domain": {
      "namespace": "production",
      "app": "api-service",
      "team": "backend",
      "version": "v1.2.0"
    }
  }
]